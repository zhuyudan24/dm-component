.icon-status
  position relative
  display inline-block
  vertical-align middle
  width 13px
  height 13px
  border-radius 50%
  box-sizing border-box

.status-red
  font-size 14px
  line-height 15px
  color #F56C6C
  .icon-status
    border 1px solid #F56C6C
    background  rgba(245, 108, 108, .4)
    &::before
      position absolute
      content ' '
      top 50%
      left 50%
      display block
      width 6px
      height 6px
      border-radius 50%
      transform translate(-50%, -50%)
      background-color #F56C6C
  .txt
    display inline-block
    vertical-align middle

